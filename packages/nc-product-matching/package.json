    "@nestjs/core": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "knex": "^3.0.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.0",
    "lodash": "^4.17.21",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "pg": "^8.11.3",
    "csv-parser": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@types/lodash": "^4.14.0",
    "@types/jest": "^29.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/pg": "^8.10.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "rimraf": "^5.0.0"
  },
  "peerDependencies": {
    "nocodb": "*"
  }
