{
  "name": "nc-product-matching",
  "version": "1.0.0",
  "description": "Product matching feature for NocoDB - allows users to search internal products and find equivalent competitor products",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rimraf dist",
    "setup-db": "node setup-database.js",
    "dump-data": "node dump-data.js",
    "migrate-real-data": "node migrate-real-data.js",
    "migrate-enhanced": "node migrate-real-data-enhanced.js",
    "run-migration": "node run-migration.js",
    "quick-setup": "node quick-setup.js",
    "start": "node server.js",
    "dev-server": "nodemon server.js"
